<nodeDefs>

    <!-- RemoteLync Node -->
    <nodeDef id="REMOTELYNC" nodeType="139" nls="RL">
        <editors />
        <sts>
            <st id="CONNECTED" editor="BOOL" /> <!-- RemoteLync is connected -->  
        </sts>
        <cmds>
            <sends />
            <accepts>
            	<cmd id="CONNECT" />
            </accepts>
        </cmds>
    </nodeDef>

	<!-- RemoteLync Smoke/CO Node -->
	<nodeDef id="SMOKE" nls="SM">
        <editors />
        <sts>
            <st id="SMOKE" editor="BOOL" /> <!-- Smoke Status -->
            <st id="CO" editor="BOOL" /> <!-- CO Status -->
        </sts>
        <cmds>
        	<sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
            </accepts>
        </cmds>
    </nodeDef>

	<!-- RemoteLync Camera Node -->
	<nodeDef id="CAMERA" nls="CAM">
        <editors />
        <sts>
            <st id="MOTION" editor="BOOL" /> <!-- Motion Status -->
        </sts>
        <cmds>
        	<sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
            </accepts>
        </cmds>
    </nodeDef>

</nodeDefs>
